<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:views="clr-namespace:MSUScripter.Views"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        x:Class="MSUScripter.Views.MainWindow"
        Icon="/Assets/MSUScripterIcon.ico"
        MinWidth="1024"
        MinHeight="768"
        Width="1024"
        Height="768"
        Loaded="Control_OnLoaded"
        Closing="Window_OnClosing"
        Title="MSUScripter">
    <LayoutTransformControl Name="MainLayout">
        <Grid Name="MainGrid" RowDefinitions="Auto,*" IsSharedSizeScope="True">
            
            <DockPanel Grid.Row="1" Name="MainPanel">
                
            </DockPanel>
            
            <DockPanel Grid.Row="1" Name="EditParent">
                <views:EditProjectPanel Name="EditProjectPanel" OnCloseProject="EditProjectPanel_OnOnCloseProject"></views:EditProjectPanel>
            </DockPanel>
            
        </Grid>
    </LayoutTransformControl>
</Window>
