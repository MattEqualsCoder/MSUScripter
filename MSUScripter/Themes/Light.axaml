<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    x:CompileBindings="True">
    
    <Color x:Key="CardBackgroundColor">#EAEAEA</Color>
    <SolidColorBrush x:Key="CardBackground" Color="{StaticResource CardBackgroundColor}"/>
    
    <Color x:Key="CardBorderColor">#C6C6C6</Color>
    <SolidColorBrush x:Key="CardBorder" Color="{StaticResource CardBorderColor}"/>
    
    <Color x:Key="LinkColor">#1151FF</Color>
    <SolidColorBrush x:Key="Link" Color="{StaticResource LinkColor}"/>
    
    <Color x:Key="FadedTextColor">#878787</Color>
    <SolidColorBrush x:Key="FadedText" Color="{StaticResource FadedTextColor}"/>
    
</ResourceDictionary>